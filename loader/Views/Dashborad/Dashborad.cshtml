
@{
    Layout = null;
}


<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
<link href="~/Content/Dashboard/jquerysctipttop.css" rel="stylesheet" />
<link href="~/Content/Dashboard/jQuery-plugin-progressbar.css" rel="stylesheet" />

<link href="~/Content/Dashboard/Nav-Tab.css" rel="stylesheet" />
<style>
    .cadre {
        padding: 20px;
        border: 1px solid #ddd;
        background: #FFF none repeat scroll 0% 0%;
        box-shadow: 0px 1px 3px rgba(0, 0, 0, 0.3);
    }

    .nav-tabs > li > a, .nav-tabs > li > a:hover {
        padding: 20px;
        width: 120px;
        font-weight: 550;
        text-transform: uppercase;
        font-size: 16px;
    }

    .progress-bar div span {
        position: absolute;
        font-family: Arial;
        font-size: 50px;
        line-height: 180.5px;
        height: 180.5px;
        width: 180.5px;
        left: 10px;
        top: 10px;
        text-align: center;
        border-radius: 50%;
        background-color: #fefef7;
    }

    .texto > h3 {
        color: #3C3E4A;
        font-weight: 600;
        font-size: 24px;
    }

    .texto > p {
        color: #C3C3C3;
        font-weight: 600;
    }

    .texto > center > h3 {
        color: #3C3E4A;
        font-weight: 600;
        font-size: 24px;
    }

    .texto > center > p {
        color: #C3C3C3;
        font-weight: 600;
    }

    .partialViewWrapper {
        height: 600px;
        width: 500px;
        /*overflow-y: auto;*/
    }

    .partialViewWrapper1 {
        height: 100%;
        width: 100%;
    }

    @@media (min-width: 1800px) .container1 {
        width: 1700px;
    }

    @@media (min-width: 1200px) .container1 {
        width: 1170px;
    }

    @@media (min-width: 992px) .container1 {
        width: 970px;
    }

    @@media (min-width: 768px) .container1 {
        width: 750px;
    }

    .container1 {
        padding-right: 15px;
        padding-left: 15px;
        margin-right: auto;
        margin-left: auto;
    }
</style>
<div class="container1">
    <div class="row">
        <div class="col-md-6">
            @{ Html.RenderAction("DashboradPartial", "Dashborad"); }
        </div>
        <div class="col-md-6">
            @{ Html.RenderAction("DashboradPartial", "Dashborad"); }
        </div>
    </div>

    <div class="row">

        <div class="col-md-6">
            @{ Html.RenderAction("DashboradPartial", "Dashborad"); }
        </div>
        <div class="col-md-6">
            @{ Html.RenderAction("DashboradPartial", "Dashborad"); }
        </div>
    </div>

</div>
@*<div class="partialViewWrapper">
        <div class="partialViewWrapper1">
            @{ Html.RenderAction("DashboradPartial", "Dashborad"); }
        </div>
    </div>*@

<script src="~/Scripts/jquery-2.1.4.min.js"></script>
<script src="~/Content/Dashboard/jQuery-plugin-progressbar.js"></script>
<script src="~/Scripts/bootstrap.min.js"></script>
<script>
    $(".progress-bar").loading();
    $('input').on('click', function () {
        $(".progress-bar").loading();
    });
</script>


